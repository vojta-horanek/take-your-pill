<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <size
                android:width="256dp"
                android:height="256dp" />
            <solid android:color="@color/colorTeal" />
        </shape>
    </item>
    <item
        android:width="100dp"
        android:height="100dp"
        android:bottom="24dp"
        android:drawable="@drawable/ic_history"
        android:end="24dp"
        android:gravity="start"
        android:start="24dp"
        android:top="24dp" />

    <item
        android:width="100dp"
        android:height="100dp"
        android:bottom="24dp"
        android:drawable="@drawable/ic_pill"
        android:end="24dp"
        android:gravity="bottom|end"
        android:start="24dp"
        android:top="24dp" />

</layer-list>


